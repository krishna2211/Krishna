@using Web_Day4.Models
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @{
        var employee = ViewData["Employee"] as Web_Day4.Models.Employee;
    }
    <h2>@ViewData["Header"]</h2>
    <table style="font-family:Arial" border="1" bgcolor="beige">
        <tr>
            <td>
                Employee ID:
            </td>
            <td>@employee.EmployeeId</td>
        </tr>
        <tr>
            <td>
                Employee Name:
            </td>
            <td>@employee.Name</td>
        </tr>
        <tr>
            <td>
                Employee Address:
            </td>
            <td>@employee.Address</td>
        </tr>
        <tr>
            <td>
                Employee City:
            </td>
            <td>@employee.City</td>
        </tr>
        <tr>
            <td>
                Employee gender:
            </td>
            <td>@employee.Gender</td>
        </tr>
        <tr>
            <td>
                Employee Salary:
            </td>
            <td>@employee.Salary</td>
        </tr>
    </table>
    <div>
    </div>
</body>
</html>
